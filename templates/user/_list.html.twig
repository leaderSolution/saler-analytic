<table id="example" class="table">
	<thead>
		<tr>
			<th>Email</th>
			<th>Visits</th>
			<th>actions</th>
		</tr>
	</thead>
	<tbody>
		{% for user in users %}
			<tr>
				<td>{{ user.email }}</td>
				<td>
					{% if user.visits is not empty  %}
						<a href="{{ path('user_visits', {'id': user.id}) }}">Visit List
						</a>
					{% else %}
						None
					{% endif %}
				</td>
				<td>
					<a href="{{ path('user_edit', {'id': user.id}) }}">
						<i class="fs-5 bi-pencil-square"></i>
					</a>
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="5">no records found</td>
			</tr>
		{% endfor %}
	</tbody>
</table>
